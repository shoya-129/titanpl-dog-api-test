{
  "html:../app/static/index.html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n  <title>Modern Tailwind + GSAP Page</title>\r\n  <link rel=\"icon\" type=\"image/png\" href=\"https://cdn.jsdelivr.net/gh/twitter/twemoji@14.0.2/assets/72x72/1f680.png\">\r\n\r\n  <!-- Tailwind CSS -->\r\n  <script src=\"https://cdn.jsdelivr.net/npm/@tailwindcss/browser@4\"></script>\r\n\r\n  <!-- GSAP CDN -->\r\n  <script src=\"https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js\"></script>\r\n</head>\r\n\r\n<body class=\"bg-gray-100 flex items-center justify-center min-h-screen\">\r\n\r\n  <div id=\"card\" class=\"max-w-xl p-10 bg-white rounded-3xl shadow-xl text-center opacity-0 translate-y-10\">\r\n\r\n    <img src=\"tpl{{dog_image}}\" class=\"mt-8 w-full rounded-2xl shadow-lg transition-opacity duration-500 opacity-0\"\r\n      onload=\"this.classList.remove('opacity-0')\" />\r\n\r\n    <a id=\"btn\" href=\"tpl{{dog_image}}\"\r\n      class=\"mt-8 px-8 py-3 text-white bg-indigo-600 hover:bg-indigo-700 rounded-xl transition-all duration-300 opacity-0\">\r\n      Click Me\r\n    </a>\r\n  </div>\r\n\r\n\r\n  <script>\r\n    // Fade + slide-in container\r\n    gsap.to(\"#card\", {\r\n      duration: 1,\r\n      opacity: 1,\r\n      y: 0,\r\n      ease: \"power3.out\"\r\n    });\r\n\r\n    // Stagger text animations\r\n    gsap.to(\"#title\", {\r\n      duration: 1,\r\n      delay: 0.3,\r\n      opacity: 1,\r\n      y: 0,\r\n      ease: \"power3.out\"\r\n    });\r\n\r\n    gsap.to(\"#paragraph\", {\r\n      duration: 1,\r\n      delay: 0.6,\r\n      opacity: 1,\r\n      y: 0,\r\n      ease: \"power3.out\"\r\n    });\r\n\r\n    gsap.to(\"#btn\", {\r\n      duration: 1,\r\n      delay: 0.9,\r\n      opacity: 1,\r\n      y: 0,\r\n      ease: \"power3.out\"\r\n    });\r\n\r\n    // Button floating effect\r\n    gsap.to(\"#btn\", {\r\n      y: -5,\r\n      repeat: -1,\r\n      yoyo: true,\r\n      duration: 1.2,\r\n      ease: \"easeInOut\"\r\n    });\r\n  </script>\r\n</body>\r\n\r\n</html>",
  "dog:image:v1": "[object Object]",
  "dog:image": "[object Object]"
}